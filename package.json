{
  "name": "vs-flux",
  "displayName": "vs-flux",
  "description": "misc stuff to be done",
  "version": "0.0.1",
  "publisher": "ranjitjhala",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-flux.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "cursorPositionContainer",
          "title": "Cursor Position",
          "icon": "path/to/icon.svg"
        }
      ]
    },
    "views": {
      "cursorPositionContainer": [
        {
          "type": "webview",
          "id": "cursorPositionView",
          "name": "Cursor Position"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}